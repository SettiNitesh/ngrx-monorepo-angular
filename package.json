{
  "name": "ngrx-mono",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:shared": "ng build shared",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "build:shell": "ng build shell --configuration production",
    "build:flights": "ng build flights --configuration production",
    "build:hospital": "ng build hospital --configuration production",
    "build:akita-counter": "ng build akita-counter --configuration production",
    "build:mfe": "ng build mfe --configuration production",
    "build:all": "npm run build:shell && npm run build:flights && npm run build:hospital && npm run build:mfe && npm run build:akita-counter",
    "deploy:gh-pages": "npm run build:all && npx gh-pages -d dist/shell -e shell && npx gh-pages -d dist/flights -e flights && npx gh-pages -d dist/hospital -e hospital && npx gh-pages -d dist/mfe -e mfe && npx gh-pages -d dist/akita-counter -e akita-counter"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^19.0.1",
    "@angular/animations": "^19.1.4",
    "@angular/cdk": "^19.1.2",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.1.2",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@datorama/akita": "^8.0.1",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@ngrx/store": "^19.0.0",
    "@ngrx/store-devtools": "^19.0.0",
    "@tailwindcss/postcss": "^4.0.3",
    "angular-cli-ghpages": "^2.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.4.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.5",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.1.0",
    "ngx-build-plus": "^19.0.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2"
  }
}
